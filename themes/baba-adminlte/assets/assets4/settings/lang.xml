  <item id="btnNeutral">{text name="btnneutral" label="btnneutral"}{/text}</item>
  <item id="btnDisagreement">{text name="btndisagreement" label="btndisagreement"}{/text}</item>
  <item id="btnTransfer">{text name="btntransfer" label="btntransfer"}{/text}</item>
  <item id="btnMail">{text name="btnMail" label="btnMail"}Send via email{/text}</item>
  <item id="btnPrint">{text name="btnprint" label="btnprint"}{/text}</item>

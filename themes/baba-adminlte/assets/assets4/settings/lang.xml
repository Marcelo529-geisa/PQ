  <item id="transferHead">{text name="transferhead" label="transferhead"}{/text}</item>
  <item id="transferText">{text name="transfertext" label="transfertext"}{/text}</item>
  <item id="transferFailed">{text name="transferfailed" label="transferfailed"}{/text}</item>
  <item id="transferOk">{text name="transferok" label="transferok"}{/text}</item>


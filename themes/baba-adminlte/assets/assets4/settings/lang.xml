<language version="1.0" htmlParse="true">

  <!-- misc -->
  <item id="btnContinue">{text name="btncontinue" label="btncontinue"}{/text}</item>
  <item id="btnclose">{text name="btnclose" label="btnclose"}{/text}</item>
  <item id="btnHelp">{text name="btnhelp" label="btnhelp"}{/text}</item>

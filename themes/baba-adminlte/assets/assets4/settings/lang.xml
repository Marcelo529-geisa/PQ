  <item id="welcomeHead">{text name="welcomehead" label="welcomehead"}{/text}</item>
  <item id="welcomeText">{text name="welcometext" label="welcometext"}{/text}</item>

  <!-- user login, only displayed if access is restricted (see config-file) -->
  <item id="loginHead">{text name="loginhead" label="loginhead"}{/text}</item>
  <item id="loginText">{text name="logintext" label="logintext"}{/text}</item>
  <item id="loginFormHeader">{text name="loginformheader" label="loginformheader"}{/text}</item>
  <item id="loginNoInput">{text name="loginnoinput" label="loginnoinput"}{/text}</item>

  <item id="introHead">{text name="introhead" label="introhead"}{/text}</item>
  <item id="introText">{text name="introtext" label="introtext"}{/text}</item>

  <!-- step1: rough sorting into three piles (leave blank to skip popup)-->
  <item id="step1Head">{text name="step1head" label="step1head"}{/text}</item>
  <item id="step1Text">{text name="step1text" label="step1text"}{/text}</item>

  <!-- step2: sorting (leave blank to skip popup) -->
  <item id="step2Head">{text name="step2head" label="step2head"}{/text}</item>
  <item id="step2Text">{text name="step2text" label="step2text"}{/text}</item>

  <!-- step3: check sorting, only displayed if showStep3== true (see config-file; leave blank to skip popup)-->
  <item id="step3Head">{text name="step3head" label="step3head"}{/text}</item>
  <item id="step3Text">{text name="step3text" label="step3text"}{/text}</item>

  <!-- step4: comments on best/worst rated statements, only displayed if showStep4== true (see config-file; leave blank to skip popup) -->
  <item id="step4Head">{text name="step4head" label="step4head"}{/text}</item>
  <item id="step4Text">{text name="step4text" label="step4text"}{/text}</item>


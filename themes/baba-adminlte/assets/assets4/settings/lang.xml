  <!-- HtmlQ only: In-App Back Button -->
  <item id="backButton">{text name="backbutton" label="backbutton"}{/text}</item>
  <item id="fillInRequiredFields">{text name="fillinrequiredfields" label="fillinrequiredfields"}{/text}</item>


  <item id="mailHead">{text name="mailhead" label="mailhead"}{/text}</item>
  <item id="mailText">{text name="mailtext" label="mailtext"}{/text}</item>

  <!-- e-mail-body -->
  <item id="mailBody">{text name="mailbody" label="mailbody"}{/text}</item>

